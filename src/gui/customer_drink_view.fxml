<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controller.CustomerDrinkController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <HBox layoutX="-1.0" prefHeight="600.0" prefWidth="600.0">
            <children>
                <ScrollPane fx:id="scrollPane" prefHeight="600.0" prefWidth="600.0">
                    <content>
                        <GridPane fx:id="gridPane" prefHeight="600.0" prefWidth="600.0">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            </rowConstraints>
                        </GridPane>
                    </content>
                </ScrollPane>
            </children>
        </HBox>
    </children>

</AnchorPane>
